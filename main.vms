label begin
    ; A
    push 10
    call fact
    jmp end

label end
    pop
    hlt

label fact
    ; B <- A
    load $-2
    ; if B < 2
    push 2
    load $-2
    jl _clfact
label _fact
    ; B <- B - 1
    push 1
    load $-2
    sub
    store $-2 $-1
    pop
    ; A <- A * B
    load $-3
    load $-2
    mul
    store $-4 $-1
    pop
    ; if B > 1
    push 1
    load $-2
    jg _fact
label _clfact
    pop
    ret
