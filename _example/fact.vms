; Factorial
label begin
    ; A <-
    push 10
    call fact
    jmp end
label end
    ; <- A
    pop
    hlt
; A <- fact(A)
label fact
    ; B <- A
    load -2
label _fact
    ; IF B < 2
    load -1
    push 2
    jl _end
    ; B <- B - 1
    load -1
    push 1
    sub
    store -2 -1
    pop
    ; A <- A * B
    load -3
    load -2
    mul
    store -4 -1
    pop
    jmp _fact
label _end
    ; return
    pop
    ret
